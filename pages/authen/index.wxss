/* pages/authen/index.wxss */

.authen{
  width: 100%;
}
.authen .ctx{
  margin: 0 20rpx;
  position: absolute;
  top: 0;
  width: 95%;
  background: #FFFFFF;
  box-shadow: 0 0 12px 2px rgba(0,0,0,0.08);
  border-radius: 4px;
}
.authen .ctx .authen-header{
  width: 100%;
  background-image: linear-gradient(-180deg, #FFFFFF 0%, #FFFFFF 51%, #CCE5D9 100%);
  border-radius: 0px 4px 0px 0px;
}
.authen .ctx .authen-header image{
  width: 100%;
  height: 440rpx;
  object-fit: cover
}
.authen-info{
  font-size: 28rpx;
}
.authen-info .authen-info-title{
  padding: 20rpx;
  border-bottom: 1px solid #eee;
}
.authen-info .yzm{
  display: flex;
  align-items: center;
  justify-content: space-between
}
.authen-info .yzm button{
  font-size: 28rpx;
}
/*checkbox 整体大小  */

/*checkbox 选项框大小  */
checkbox .wx-checkbox-input {
  width: 30rpx;
  height: 30rpx;
}
/*checkbox选中后样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background: #07c160;
}
/*checkbox选中后图标样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
.picker{
  width: 300rpx
  }
  .get-yzm{
    width: 150rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    background: #2ABF77;
    color: #fff;
    font-size: 24rpx;
    border-radius: 4rpx;
  }
