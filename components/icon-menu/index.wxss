/* components/iconMenu/index.wxml.wxss */
.icon-menu-item{
  width: auto;
  text-align: center;
  margin: 20rpx 0;
}
.icon-menu-item .images {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx;
  overflow: hidden;
  margin: 0 auto;
}

.icon-menu-item .images image {
  width: 100%;
  height: 100%;
}

.icon-menu-item text {
  font-size: 28rpx;
}
