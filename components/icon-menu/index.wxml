<!--components/iconMenu/index.wxml.wxml-->
<view class="icon-menu-item" style="width:{{width}}">
  <view bindtap="gotoPage"  data-url="{{data.url}}"  class="images" style="width:{{iconWidth}};height:{{iconHeight}}">
    <image wx:if="{{data.src}}" src="{{data.src}}"></image>
    <view wx:if="{{data.iconfont}}" class="iconfont {{data.iconfont}} item-icon"></view> 
  </view>
  <text>{{data.label}}</text>
  <view wx:if="{{data.desc}}" class="desc">{{data.desc}}</view>
</view>